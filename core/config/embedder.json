{
  "embedder": {
    "client_class": "DashScopeEmbedder",
    "batch_size": 10,
    "model_kwargs": {
      "model": "text-embedding-v4",
      "dimensions": 256,
      "encoding_format": "float"
    }
  },
  "sketch_filling": true,
  "force_embedding": true,
  "retriever": {
    "top_k": 20
  },
  "hybrid": {
    "enabled": true,
    "bm25": {
      "top_k": 100,
      "k1": 1.2,
      "b": 0.75
    }
  },
  "text_splitter": {
    "split_by": "word",
    "chunk_size": 512,
    "chunk_overlap": 32
  }
}